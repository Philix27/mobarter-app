    "format": "prettier --write .",
    "test:integration": "playwright test",
    "prepare": "husky",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "husky": "^9.1.6",
    "typescript": "^5.0.0"
  },
  "type": "module"
}
